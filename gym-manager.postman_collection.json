  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Google Auth",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test(\"Successful authentication\", function() {",
                  "    pm.response.to.have.status(200);",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.success).to.be.true;",
                  "    pm.expect(jsonData.data).to.have.property('token');",
                  "    if (jsonData.data.token) {",
                  "        pm.environment.set('authToken', jsonData.data.token);",
                  "        console.log('Auth token updated:', jsonData.data.token);",
                  "    } else {",
                  "        console.log('No token found in response.');",
                  "    }",
                  "});"
                  
                  "if (pm.environment.get('authToken')) {",
                  "    pm.request.headers.upsert({ key: 'Authorization', value: 'Bearer ' + pm.environment.get('authToken') });",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"firebaseToken\": \"<firebase_id_token>\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/google",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "google"]
            }
          }
        }
      ]
    },
